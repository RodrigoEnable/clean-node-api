{
  "name": "clean-node-api",
  "version": "1.0.0",
  "description": "by rodrigo manguinho",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "jest --watch --silent --verbose --colors --noStackTrace",
    "test:staged": "jest --passWithNoTests --findRelatedTests --silent --noStackTrace",
    "test:ci": "jest --coverage --silent --noStackTrace"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/RodrigoEnable/clean-node-api.git"
  },
  "lint-staged": {
    "*.js": [
      "npm standard --fix",
      "npm run test:staged --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci --silent --noStackTrace"
    }
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/RodrigoEnable/clean-node-api/issues"
  },
  "homepage": "https://github.com/RodrigoEnable/clean-node-api#readme",
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "husky": "^5.0.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard": "^16.0.3",
    "sucrase": "^3.17.1"
  }
}
